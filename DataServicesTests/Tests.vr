        Query.Args.Inputs.Query      = "CMCUSTNO = {CMCUSTNO}"
        Query.Args.Inputs.QueryFields.Add(*New ASNA.Helpers.DataServices.QueryField("CMCUSTNO",Id))

        dt = Query.Exec() *As DataTable 
        db.Close()
    EndSr

